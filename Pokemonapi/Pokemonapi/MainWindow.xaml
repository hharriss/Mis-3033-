<Window x:Class="Pokemonapi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pokemonapi"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="lstnames" HorizontalAlignment="Left" Height="184" Margin="43,101,0,0" VerticalAlignment="Top" Width="209"/>
        <Button x:Name="Btnallpokemon" Content="Find all pokemon" HorizontalAlignment="Left" Margin="422,39,0,0" VerticalAlignment="Top" Width="139" Height="58" Click="Btnallpokemon_Click"/>
        <Button x:Name="BtnAbility" Content="Find ability" HorizontalAlignment="Left" Margin="422,292,0,0" VerticalAlignment="Top" Width="133" Height="65" Click="BtnAbility_Click"/>
        <TextBlock x:Name="txtblock" HorizontalAlignment="Left" Margin="422,159,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="126" Width="197"/>

    </Grid>
</Window>
